{
    "name": "urbaton",
    "version": 2,
    "builds": [
        {
            "src": "client/package.json",
            "use": "@now/static-build",
            "config": {
                "distDir": "build"
            }
        }
    ],
    "routes": [
        {
            "src": "/client/(.*)",
            "dest": "/$1"
        }
    ]
}
