{
    "name": "urbaton",
    "version": 2,
    "builds": [
        {
            "src": "client/package.json",
            "use": "@now/static-build",
            "config": {
                "distDir": "build"
            }
        },
        {
            "src": "server/*.py",
            "use": "@now/python"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/server/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/client/$1"
        }
    ]
}
